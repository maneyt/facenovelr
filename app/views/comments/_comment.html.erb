<div class="individual-comment"> 
  <div>
    <div class="feed_profile_images">
    <%= link_to image_tag(comment.user.profile_picture.url(:thumbnail)), user_show_path(comment.user) %>
  </div>
    <div> <%= link_to comment.user.name, user_show_path(comment.user) %> </div>
    <div>
      <%= comment.body %>
    </div>
    <div class="information">
      <%= time_ago_in_words(comment.created_at) %> ago
    </div>
</div>
